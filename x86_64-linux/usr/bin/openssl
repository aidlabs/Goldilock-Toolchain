#!/bin/bash
realpath=`readlink -fn $0`
export OPENSSL_CONF=/home/simon/work/goldilocks/tmp/sysroots/x86_64-linux/usr/lib/ssl/openssl.cnf SSL_CERT_DIR=/home/simon/work/goldilocks/tmp/sysroots/x86_64-linux/usr/lib/ssl/certs SSL_CERT_FILE=/home/simon/work/goldilocks/tmp/sysroots/x86_64-linux/usr/lib/ssl/cert.pem OPENSSL_ENGINES=/home/simon/work/goldilocks/tmp/sysroots/x86_64-linux/usr/lib/ssl/engines
exec -a `dirname $realpath`/openssl `dirname $realpath`/openssl.real "$@"
